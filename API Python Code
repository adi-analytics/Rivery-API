

import pandas as pd   
import requests
import json

url = 'https://console.rivery.io/api/rivers'
payload={}
headers = {
"Content-Type": "application/json",
"Authorization": "Bearer token"}


response = requests.request("GET", url, headers=headers, data=payload)

rivery_data = response.json()

